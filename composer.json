{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.3.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-migrations-bundle": "^3.5",
        "doctrine/orm": "^2.15",
        "fakerphp/faker": "^1.21",
        "google/recaptcha": "^1.3",
        "paragonie/sodium_compat": "^1.21",
        "phpdocumentor/reflection-docblock": "^5.6",
        "symfony/asset": "^7.3",
        "symfony/console": "^7.3",
        "symfony/dotenv": "^7.3",
        "symfony/event-dispatcher": "^7.3",
        "symfony/expression-language": "^7.3",
        "symfony/flex": "^2.3",
        "symfony/form": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/http-client": "^7.3",
        "symfony/intl": "^7.3",
        "symfony/mailer": "^7.3",
        "symfony/mime": "^7.3",
        "symfony/monolog-bundle": "^3.9",
        "symfony/notifier": "^7.3",
        "symfony/process": "^7.3",
        "symfony/property-access": "^7.3",
        "symfony/property-info": "^7.3",
        "symfony/security-bundle": "^7.3",
        "symfony/security-csrf": "^7.3",
        "symfony/serializer": "^7.3",
        "symfony/string": "^7.3",
        "symfony/translation": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/validator": "^7.3",
        "symfony/web-link": "^7.3",
        "symfony/webpack-encore-bundle": "^2.3",
        "symfony/yaml": "^7.3",
        "tinymce/tinymce": "^6.7",
        "twig/extra-bundle": "^3.16",
        "twig/twig": "^3.16"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.3",
        "doctrine/doctrine-fixtures-bundle": "^4.1",
        "friendsofphp/php-cs-fixer": "^3.61",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpstan/phpdoc-parser": "^2.0",
        "phpunit/phpunit": "^10.5",
        "psalm/plugin-symfony": "^5.0",
        "symfony/browser-kit": "^7.3",
        "symfony/css-selector": "^7.3",
        "symfony/debug-bundle": "^7.3",
        "symfony/maker-bundle": "^1.52",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/stopwatch": "^7.3",
        "symfony/var-dumper": "^7.3",
        "symfony/web-profiler-bundle": "^7.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "psalm": [
            "php vendor/vimeo/psalm/psalm --no-cache --no-reflection-cache --no-file-cache"
        ],
        "csfixer": [
            "php vendor/friendsofphp/php-cs-fixer/php-cs-fixer check --config=\".php-cs-fixer.php\" --show-progress=\"none\""
        ],
        "phpStan": [
            "php vendor/phpstan/phpstan/phpstan analyse src tests --memory-limit=256M"
        ],
        "tests-unit": [
            "php vendor/phpunit/phpunit/phpunit tests/Unit"
        ],
        "quality-check": [
            "@csfixer",
            "@psalm",
            "@phpStan",
            "@tests-unit"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.3"
        }
    }
}
